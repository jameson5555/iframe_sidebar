{
	"manifest_version": 2,
	"name": "Iframe Sidebar",
	"version": "1.0",
	"description": "Inserts a sidebar with a variable path into the current page",
	"permissions": [
    	"activeTab"
	],
	"background": {
		"scripts": ["jquery.js", "background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"web_accessible_resources": [
		"styles.css"
	]
}
